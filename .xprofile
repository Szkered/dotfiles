# Keymap
setxkbmap \
	-model pc105 \
	-layout us \
	-variant altgr-intl \
	-option terminate:ctrl_alt_bksp \
	-option ctrl:nocaps \
    -option ctrl:swap_lwin_lctl \

xcape -e 'Control_L=Escape'

# Key repeat
xset r rate 250 60

# Wallpaper
nitrogen --restore &

# Compositor
picom -f &

# Input method
fcitx5 &
